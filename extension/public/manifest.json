{
    "name": "iRefined",
	"version": "0.0.0",
	"description": "Improves the iRacing UI",
	"manifest_version": 3,
    "content_scripts": [
		{
			"run_at": "document_start",
			"matches": [
				"https://*.iracing.com/*"
			],
			"exclude_matches": [
				"https://oauth.iracing.com/*"
			],
			"js": [
				"bootstrap.js"
			]
		}
	]
}
